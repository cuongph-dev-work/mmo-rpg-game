{
  "name": "mmo-rpg-backend",
  "version": "1.0.0",
  "description": "Backend services for MMO RPG Game",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "npm run build -ws",
    "start:dev": "npm run start:dev -w",
    "start:prod": "npm run start:prod -w",
    "lint": "eslint \"{apps,libs}/**/*.ts\"",
    "format": "prettier --write \"{apps,libs}/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.22",
    "@nestjs/platform-ws": "^10.4.22",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/websockets": "^10.4.22",
    "@prisma/client": "^5.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bufferutil": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ioredis": "^5.9.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "utf-8-validate": "^6.0.6",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
